{
  "$schema": "https://json.schemastore.org/launch",
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch (Windows MinGW gdb) Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/win-gcc-debug/cpp_starter.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "PATH",
          "value": "C:\\mingw64\\bin;${env:PATH}"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\mingw64\\bin\\gdb.exe",
